/* Pick from a table */
let table = base.getTable("砖驻转 转转 ");
let record = await input.recordAsync('Pick a record', table);
if (record) {
    //import fields records
    console.log(`You picked ${record.getCellValueAsString("拽 转 ()")}`);
   

 
    
    //Get Records fields 
    let response = await fetch('WEBHOOK URL ADDRESS', {
        method: 'POST',
        body: JSON.stringify({
            "FullName": record.getCellValueAsString('砖 '),
            "Phone": record.getCellValueAsString('驻'), 
            "City": record.getCellValueAsString('注专'),
            "Adress":record.getCellValueAsString('转转'),
            "RecordId" : record.id
     
        }),
        headers:{
            'Content-Type' : 'application/json'
        } 
    });
    }
